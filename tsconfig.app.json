{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": []
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ],
  "paths":{
    "@views/*":[
      "src/app/views/hogwarts"
    ],
    "@services/*":[
      "src/app/app_core/services"
    ],
  }

}
